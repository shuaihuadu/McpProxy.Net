{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "McpProxy": "Debug"
    }
  },
  "AllowedHosts": "*",
  "HttpServer": {
    "Host": "0.0.0.0",
    "Port": 8080,
    "Stateless": false,
    "AllowedOrigins": ["*"]
  },
  "McpServers": [
    {
      "Name": "filesystem",
      "Command": "npx",
      "Arguments": ["-y", "@modelcontextprotocol/server-filesystem", "C:\\Users"],
      "Environment": {
        "NODE_ENV": "production"
      },
      "WorkingDirectory": null,
      "Enabled": true
    },
    {
      "Name": "github",
      "Command": "npx",
      "Arguments": ["-y", "@modelcontextprotocol/server-github"],
      "Environment": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "ghp_your_token_here"
      },
      "WorkingDirectory": null,
      "Enabled": true
    },
    {
      "Name": "fetch",
      "Command": "uvx",
      "Arguments": ["mcp-server-fetch"],
      "Environment": {},
      "WorkingDirectory": null,
      "Enabled": true
    }
  ],
  "UseNamespacePrefix": true,
  "AllowServerFilter": true,
  "AutoReconnect": true,
  "HealthCheckInterval": 30
}
